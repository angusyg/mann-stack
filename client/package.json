    "format:tests": "prettier --write \"e2e/**/*.{ts,js}\"",
    "ng": "ng",
    "start": "ng serve",
    "lint": "ng lint",
    "test": "ng test",
    "test:e2e": "ng e2e"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/angusyg/mann-stack-client.git"
  },
  "keywords": [
    "angular",
    "typescript",
    "sass",
    "jasmine",
    "protractor",
    "karma"
  ],
  "bugs": {
    "url": "https://github.com/angusyg/mann-stack-client/issues"
  },
  "homepage": "https://github.com/angusyg/mann-stack-client#readme",
  "dependencies": {
    "@angular/animations": "~7.0.0",
    "@angular/common": "~7.0.0",
    "@angular/compiler": "~7.0.0",
    "@angular/core": "~7.0.0",
    "@angular/forms": "~7.0.0",
    "@angular/http": "~7.0.0",
    "@angular/platform-browser": "~7.0.0",
    "@angular/platform-browser-dynamic": "~7.0.0",
    "@angular/router": "~7.0.0",
    "core-js": "^2.5.4",
    "rxjs": "~6.3.3",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.10.0",
    "@angular/cli": "~7.0.6",
