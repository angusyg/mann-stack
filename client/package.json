  "description": "Mann Stack (Mongoose, Angular, Nestjs, Nodejs) client starter",
  "license": "MIT",
  "author": "Angusyg",
  "private": true,
  "scripts": {
    "build": "ng build",
    "format": "npm run format:src && npm run format:tests",
